# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# API Keys
HUNTER_IO_API_KEY=your-hunter-api-key
PERPLEXITY_API_KEY=your-perplexity-api-key

# Service Configuration
LOG_LEVEL=INFO
MAX_WORKERS=5
BATCH_SIZE=10
REQUEST_TIMEOUT=30

# Processing Limits
MAX_EMAILS_PER_DOMAIN=5
HUNTER_CONFIDENCE_THRESHOLD=70
VERIFICATION_CONFIDENCE_THRESHOLD=80

# Rate Limiting (requests per minute)
HUNTER_RATE_LIMIT=10
PERPLEXITY_RATE_LIMIT=30

# Database Configuration
DB_CONNECTION_POOL_SIZE=10
DB_CONNECTION_TIMEOUT=30

# Caching (Redis - optional)
# REDIS_URL=redis://localhost:6379
# CACHE_TTL=3600

# Development
DEBUG_MODE=false
DRY_RUN=false

# Skip existing emails during processing
SKIP_EXISTING=true
